/* 美食列表开始 */
.list-nav-container{
  width: 100%;
  height: auto;
  margin: 0  15rpx;
  border-radius: 10rpx; 

}

.list-nav{
  width: calc(100% - 30rpx);
  height: 80rpx;
  border-radius: 10rpx 10rpx 0 0;
  /* margin: 0 auto; */
  background: #f5f5f5;
 
}

.nav-item{
  position: relative;
  width: calc(720rpx / 4);
  height: 80rpx;
  line-height: 80rpx;
  font-size: 20rpx;
  text-align: center;
  color: #888;
}

.nav-item-active{
  height: 88rpx;
  width: calc((720rpx / 4) + 8rpx);
  position: absolute;
  top: -6rpx;
  color: #333;
  background:#fff;
  border-radius: 0 20rpx 0 0;
  font-weight: blod;
}

.left{
  left: -10rpx;
}

.inline{
  position: relative;
  display: inline-flex;
}

.nav-tip{
  position: absolute;
  top: 36rpx;
  left: 0;
  font-size: 12.6rpx;
  color: #b7b7b7;
}

.nav-column::after{
  content: '|';
  position: absolute;
  right: 0;
  color: gray;
}



.border-arrow{
    width: 0;
    height: 0;
    position: absolute;
    right: -60rpx;
    top: -6rpx;
    z-index: 20;
    border-top: 27rpx solid;
    border-right: 78rpx solid;
    border-left: 35rpx solid transparent;
    border-bottom: 57rpx solid transparent;
    border-color: transparent transparent #fff transparent;
    transform: rotateZ(45deg);
  
}

