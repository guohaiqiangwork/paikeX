page {
  width: 100%;
  height: 100%;
  background: linear-gradient(to bottom, #fe912c, #fe733a);
  /* background: url('data:image/jpg;base64,') repeat; */
}

.container {
  display: flex;
  flex-direction: column;
}

.topbg {
  position: relative;
  z-index: 1;
  width: 100%;
  height: 1338rpx;
}

/* 活动倒计时 */

.activity-countdown {
  z-index: 2;
  position: absolute;
  bottom: 194rpx;
  left: 72rpx;
}

