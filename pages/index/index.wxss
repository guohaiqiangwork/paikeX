}

/* 活动动态 */

.activity-dynamic-container {
  margin-top: 40rpx;
  padding: 0 15rpx;
}

.activity-dynamic {
  display: flex;
  flex-direction: column;
  border-radius: 20rpx;
  width: 100%;
  height: 360rpx;
  background-color: #fffaf5;
}

.activity-dynamic-head {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  height: 120rpx;
}

.activity-dynamic-decorate {
  width: 30rpx;
  height: 30rpx;
  margin: 0 28rpx;
}

.activity-dynamic-title {
  font-size: 34rpx;
  color: #565654;
  line-height: 120rpx;
}

.activity-dynamic-user-item {
  display: flex;
  justify-content: center;
  align-items: center;
}

.activity-dynamic-user-img {
  width: 62rpx;
  height: 62rpx;
  margin: 0 10rpx;
  border-radius: 50%;
}

.activity-dynamic-user-name {
  margin-left: 16rpx;
  color: #b5b0ae;
  font-size: 24rpx;
}

.activity-dynamic-signup {
  margin-top: 50rpx;
  font-size: 26rpx;
  color: #7c381d;
  text-align: center;
}

